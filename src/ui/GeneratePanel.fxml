            <Image url="@../../img/image-5.jpg" />
         </image>
      </ImageView>
      <Label layoutX="187.0" layoutY="124.0" text="Type the number of people you want to generate" textFill="#323232">
         <font>
            <Font size="15.0" />
         </font>
      </Label>
      <TextField fx:id="numberGenerateText" layoutX="275.0" layoutY="198.0" style="-fx-background-color: transparent; -fx-border-color: black; -fx-text-fill: gray;" text="700000" />
